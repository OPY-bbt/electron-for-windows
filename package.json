{
  "name": "electron-quick-start",
  "version": "1.0.6",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist:mac": "electron-builder",
    "dist:windows": "electron-builder --win --x64"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "~13.6.6",
    "electron-builder": "^19.42.1"
  },
  "build": {
    "publish": [
      {
        "provider": "generic",
        "url": "http://192.168.20.20:8080/"
      }
    ],
    "appId": "com.electron.app",
    "productName": "survey",
    "copyright": "cx",
    "mac": {
      "category": "your.app.category.type"
    },
    "nsis": {
      "perMachine": true
    }
  },
  "dependencies": {
    "lazy-val": "^1.0.2",
    "electron-log": "^2.2.13",
    "electron-updater": "^2.18.0"
  }
}
